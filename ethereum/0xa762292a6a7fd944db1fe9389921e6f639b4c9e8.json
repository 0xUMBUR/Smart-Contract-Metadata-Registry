alSupply()":{"details":"See {IERC20-totalSupply}."},"trackFullPricePerShare()":{"details":"Emit event tracking current full price per shareProvides a pure on-chain way of approximating APY"},"transfer(address,uint256)":{"details":"Add blockLock to transfers, users cannot transfer tokens in the same block as a deposit or withdrawal."},"transferFrom(address,address,uint256)":{"details":"See {IERC20-transferFrom}. Emits an {Approval} event indicating the updated allowance. This is not required by the EIP. See the note at the beginning of {ERC20}; Requirements: - `sender` and `recipient` cannot be the zero address. - `sender` must have a balance of at least `amount`. - the caller must have allowance for ``sender``'s tokens of at least `amount`."}},"version":1},"userdoc":{"kind":"user","methods":{"available()":{"notice":"Defines how much of the Setts' underlying can be borrowed by the Strategy for useCustom logic in here for how much the vault allows to be borrowedSets minimum required on-hand to keep small withdrawals cheap"},"balance()":{"notice":"Return the total balance of the underlying token within the systemSums the balance in the Sett, the Controller, and the Strategy"},"deposit(uint256)":{"notice":"Deposit assets into the Sett, and return corresponding shares to the userOnly callable by EOA accounts that pass the _defend() check"},"deposit(uint256,bytes32[])":{"notice":"Deposit variant with proof for merkle guest list"},"depositAll()":{"notice":"Convenience function: Deposit entire balance of asset into the Sett, and return corresponding shares to the userOnly callable by EOA accounts that pass the _defend() check"},"depositAll(bytes32[])":{"notice":"DepositAll variant with proof for merkle guest list"},"depositFor(address,uint256)":{"notice":"Deposit assets into the Sett, and return corresponding shares to the userOnly callable by EOA accounts that pass the _defend() check"},"depositFor(address,uint256,bytes32[])":{"notice":"Deposit variant with proof for merkle guest list"},"earn()":{"notice":"Transfer the underlying available to be claimed to the controllerThe controller will deposit into the Strategy for yield-generating activitiesPermissionless operation"},"harvest(address,uint256)":{"notice":"Used to swap any borrowed reserve over the debt limit to liquidate to 'token'Only controller can trigger harvests"},"setController(address)":{"notice":"Change controller addressCan only be changed by governance"},"setGovernance(address)":{"notice":"Change governance addressCan only be changed by governance itself"},"setGuardian(address)":{"notice":"Change guardian addressCan only be changed by governance"},"setGuestList(address)":{"notice":"===== Permissioned Actions: Governance ====="},"setKeeper(address)":{"notice":"Change keeper addressCan only be changed by governance itself"},"setMin(uint256)":{"notice":"Set minimum threshold of underlying that must be deposited in strategyCan only be changed by governance"},"setStrategist(address)":{"notice":"Change strategist addressCan only be changed by governance itself"},"version()":{"notice":"===== View Functions ====="},"withdraw(uint256)":{"notice":"No rebalance implementation for lower fees and faster swaps"},"withdrawAll()":{"notice":"Convenience function: Withdraw all shares of the sender"}},"version":1}},"settings":{"compilationTarget":{"contracts/SettV3.sol":"SettV4"},"evmVersion":"istanbul","libraries":{},"metadata":{"bytecodeHash":"ipfs"},"optimizer":{"enabled":true,"runs":200},"remappings":[]},"sources":{"contracts/SettV3.sol":{"keccak256":"0x2e1cd2cd15b8bf1cfbe30f8be93533004b59c25341e40bdd07473e2ddfea0235","license":"MIT","urls":["bzz-raw://3f245d0a0752001c8a70280d088bcfb1c82cd6c15aeff273dca015c748758327","dweb:/ipfs/QmNwJpT7VWmJ5ifioJxzJWGwssXiRD4f5zhhYuZbbojEDB"]}},"version":1},"version":1,"checkPoints":[]}