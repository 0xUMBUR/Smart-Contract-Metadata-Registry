},"decreaseAllowance(address,uint256)":{"details":"Atomically decreases the allowance granted to `spender` by the caller. This is an alternative to {approve} that can be used as a mitigation for problems described in {IERC20-approve}. Emits an {Approval} event indicating the updated allowance. Requirements: - `spender` cannot be the zero address. - `spender` must have allowance for the caller of at least `subtractedValue`."},"delegateAllowance(address,address)":{"details":"Delegate call to get allowance","params":{"owner":"account owner","spender":"account to check allowance for"},"returns":{"_0":"allowance"}},"delegateApprove(address,uint256,address)":{"details":"Delegate call to approve","params":{"origSender":"original msg.sender on delegate contract","spender":"account to approve for","value":"amount to approve"},"returns":{"_0":"success"}},"delegateBalanceOf(address)":{"details":"Delegate call to get balance","params":{"who":"Address to get balance for"},"returns":{"_0":"balance of account"}},"delegateDecreaseApproval(address,uint256,address)":{"details":"Delegate call to decrease approval","params":{"origSender":"original msg.sender on delegate contract","spender":"spender to decrease approval for","subtractedValue":"value to subtract from approval"},"returns":{"_0":"success"}},"delegateIncreaseApproval(address,uint256,address)":{"details":"Delegate call to increase approval","params":{"addedValue":"amount of approval to add","origSender":"original msg.sender on delegate contract","spender":"account to increase approval for"},"returns":{"_0":"success"}},"delegateTotalSupply()":{"details":"Delegate call to get total supply","returns":{"_0":"Total supply"}},"delegateTransfer(address,uint256,address)":{"details":"Delegate call to transfer","params":{"origSender":"original msg.sender on delegate contract","to":"address to transfer to","value":"amount to transfer"},"returns":{"_0":"success"}},"delegateTransferFrom(address,address,uint256,address)":{"details":"Delegate call to transfer from","params":{"from":"account to transfer funds from","origSender":"original msg.sender on delegate contract","to":"account to transfer funds to","value":"value to transfer"},"returns":{"_0":"success"}},"increaseAllowance(address,uint256)":{"details":"Atomically increases the allowance granted to `spender` by the caller. This is an alternative to {approve} that can be used as a mitigation for problems described in {IERC20-approve}. Emits an {Approval} event indicating the updated allowance. Requirements: - `spender` cannot be the zero address."},"mint(address,uint256)":{"details":"Creates `amount` tokens and assigns them to `account`, increasing the total supply.","params":{"account":"address to mint tokens for","amount":"amount of tokens to be minted Emits a {Mint} event Requirements - `account` cannot be the zero address. - `account` cannot be blacklisted. - `account` cannot be a redemption address."}},"reclaimEther(address)":{"details":"send all eth balance in the contract to another address","params":{"_to":"address to send eth balance to"}},"reclaimToken(address,address)":{"details":"send all token balance of an arbitrary erc20 token in the contract to another address","params":{"_to":"address to send eth balance to","token":"token to reclaim"}},"setBlacklisted(address,bool)":{"details":"Set blacklisted status for the account.","params":{"_isBlacklisted":"blacklist flag value Requirements: - `msg.sender` should be owner.","account":"address to set blacklist flag for"}},"setBurnBounds(uint256,uint256)":{"details":"Change the minimum and maximum amount that can be burned at once. Burning may be disabled by setting both to 0 (this will not be done under normal operation, but we can't add checks to disallow it without losing a lot of flexibility since burning could also be as good as disabled by setting the minimum extremely high, and we don't want to lock in any particular cap for the minimum)","params":{"_max":"maximum amount that can be burned at once","_min":"minimum amount that can be burned at once"}},"setCanBurn(address,bool)":{"details":"Set canBurn status for the account.","params":{"_canBurn":"canBurn flag value Requirements: - `msg.sender` should be owner.","account":"address to set canBurn flag for"}},"totalSupply()":{"details":"See {IERC20-totalSupply}."},"transfer(address,uint256)":{"details":"See {IERC20-transfer}. Requirements: - `recipient` cannot be the zero address. - the caller must have a balance of at least `amount`."},"transferFrom(address,address,uint256)":{"details":"See {IERC20-transferFrom}. Emits an {Approval} event indicating the updated allowance. This is not required by the EIP. See the note at the beginning of {ERC20}; Requirements: - `sender` and `recipient` cannot be the zero address. - `sender` must have a balance of at least `amount`. - the caller must have allowance for ``sender``'s tokens of at least `amount`."},"transferOwnership(address)":{"details":"Allows the current owner to set the pendingOwner address.","params":{"newOwner":"The address to transfer ownership to."}}},"title":"TrueUSD"},"userdoc":{"methods":{}}},"settings":{"compilationTarget":{"tusd.sol":"TrueUSD"},"evmVersion":"istanbul","libraries":{},"metadata":{"bytecodeHash":"ipfs"},"optimizer":{"enabled":true,"runs":200},"remappings":[]},"sources":{"tusd.sol":{"keccak256":"0xbbecfc1a4febc1860db739e727d2b6dbec00df38e9541e389834a642ffa81a43","urls":["bzz-raw://a2bb0a39d67e2998f467a81e302b17931e8a9e84e8ddc3de48ffe8c1625abab8","dweb:/ipfs/QmPJH6mZHutDECJCFXr9Vgsy7oU8PW9uJfrVgosgRERX7t"]}},"version":1},"version":1,"checkPoints":[]}